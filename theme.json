{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": true,
			"defaultPalette": true,
			"duotone": [],
			"gradients": [
				{
					"name": "Primary Gradient",
					"slug": "primary-gradient",
					"gradient": "linear-gradient(135deg, #667eea 0%, #764ba2 100%)"
				},
				{
					"name": "Success Gradient",
					"slug": "success-gradient",
					"gradient": "linear-gradient(135deg, #11998e 0%, #38ef7d 100%)"
				}
			],
			"link": true,
			"palette": [
				{
					"name": "Primary Blue",
					"slug": "primary-blue",
					"color": "#1e40af"
				},
				{
					"name": "Secondary Teal",
					"slug": "secondary-teal",
					"color": "#0d9488"
				},
				{
					"name": "Accent Green",
					"slug": "accent-green",
					"color": "#16a34a"
				},
				{
					"name": "Dark Slate",
					"slug": "dark-slate",
					"color": "#1e293b"
				},
				{
					"name": "Light Gray",
					"slug": "light-gray",
					"color": "#f8fafc"
				},
				{
					"name": "White",
					"slug": "white",
					"color": "#ffffff"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"operator": "*",
				"increment": 0.25,
				"mediumStep": 1,
				"unit": "rem",
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "0.5rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "2rem"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fontFamilies": [
				
				{
					"name": "System Font Stack",
					"slug": "system-font-stack",
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"slug": "small",
					"size": "0.875rem",
					"fluid": false
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1rem",
					"fluid": false
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.125rem",
					"fluid": false
				},
				{
					"name": "Extra Large",
					"slug": "x-large",
					"size": "1.25rem",
					"fluid": false
				}
			],
			"lineHeight": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0.375rem"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				}
			},
			"core/group": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--medium)",
						"right": "var(--wp--preset--spacing--medium)",
						"bottom": "var(--wp--preset--spacing--medium)",
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"core/list": {
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--large)"
					}
				}
			},
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.6"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"core/pullquote": {
				"border": {
					"width": "0 0 0 4px"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--medium)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "italic"
				}
			},
			"core/quote": {
				"border": {
					"width": "0 0 0 4px"
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--medium)"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-slate)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0.375rem"
				},
				"color": {
					"background": "var(--wp--preset--color--primary-blue)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--dark-slate)",
						"text": "var(--wp--preset--color--white)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "600",
					"lineHeight": "1.2"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600",
					"lineHeight": "1.3"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "500",
					"lineHeight": "1.3"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500",
					"lineHeight": "1.4"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary-blue)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--secondary-teal)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"top": "var(--wp--preset--spacing--medium)",
				"right": "var(--wp--preset--spacing--medium)",
				"bottom": "var(--wp--preset--spacing--medium)",
				"left": "var(--wp--preset--spacing--medium)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font-stack)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-full-width",
			"title": "Page (Full Width)",
			"postTypes": [
				"page"
			]
		}
	],
	"patterns": [
		"hero-section",
		"cta-section",
		"testimonial-section"
	]
}